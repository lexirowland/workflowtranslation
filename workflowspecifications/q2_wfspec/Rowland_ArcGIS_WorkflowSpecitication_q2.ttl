@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix xml: <http://www.w3.org/XML/1998/namespace>.
@prefix wf: <http://geographicknowledge.de/vocab/Workflow.rdf#>.
@prefix tools: <http://geographicknowledge.de/vocab/GISTools.rdf#>.
@prefix arcpro: <https://pro.arcgis.com/en/pro-app/tool-reference/>.
@prefix pdok: <https://www.pdok.nl/introductie/-/article/>.

# @author: Lexi Rowland

# Workflow: proportion of elderly people living in each PC4 area in Amsterdam.
# Workflow metadata (result and data sources)
_:wf2 a wf:Workflow;
	rdfs:comment "What is the proportion of elderly people living in each PC4 area in Amsterdam?"@en;	
	wf:source <https://maps.amsterdam.nl/open_geodata/?k=192>; #postcode areas	
	wf:source <https://www.cbs.nl/nl-nl/dossier/nederland-regionaal/geografische-data/natuur-en-milieu/bestand-bodemgebruik>; #cbs buurt statistics with percentages of elderly people by neighbourhood
	wf:edge _:wf2_1, _:wf2_2, _:wf2_3, _:wf2_4, _:wf2_5, _:wf2_6, _:wf2_7.
	
_:wf2_1 wf:applicationOf <https://desktop.arcgis.com/en/arcmap/latest/tools/analysis-toolbox/identity.htm>;
		wf:input1 <https://maps.amsterdam.nl/open_geodata/?k=192>; #lattice, vectorA, nominalA
		wf:input2 <https://www.cbs.nl/nl-nl/dossier/nederland-regionaal/geografische-data/natuur-en-milieu/bestand-bodemgebruik>; #lattice, vectorA, nominalA
		wf:output _:clipped. #lattice, vectorA, nominalA
		
_:wf2_2 wf:applicationOf <https://desktop.arcgis.com/en/arcmap/latest/tools/data-management-toolbox/add-geometry-attributes.htm>; #adding area
		wf:input1 _:clipped;
		wf:output _:clipped_1. #lattice, vector, ratioA 

_:wf2_3 wf:applicationOf <https://desktop.arcgis.com/en/arcmap/10.3/tools/data-management-toolbox/add-field.htm>;
		wf:input1 _:clipped_1;
		wf:output _:clipped_shp. #lattice, vector, ratioA

_:wf2_4 wf:applicationOf <https://desktop.arcgis.com/en/arcmap/latest/tools/data-management-toolbox/calculate-field.htm>;
		wf:input1 _:clipped_shp;
		wf:output _:clipped_shp2. #lattice, vector, ratioA
		
_:wf2_5 wf:applicationOf <https://desktop.arcgis.com/en/arcmap/10.3/tools/data-management-toolbox/dissolve.htm>; #using tools:DissolveSumRatio
		wf:input1 _:clipped_shp2;
		wf:output _:diss. #lattice, vector, ratioA
		
_:wf2_6 wf:applicationOf <https://desktop.arcgis.com/en/arcmap/10.3/tools/data-management-toolbox/add-field.htm>;
		wf:input1 _:diss;
		wf:output _:diss2. #lattice, vector, ratioA

_:wf2_7 wf:applicationOf <https://desktop.arcgis.com/en/arcmap/latest/tools/data-management-toolbox/calculate-field.htm>;
		wf:input1 _:diss2;
		wf:output _:diss3. #lattice, vector, ratioA


