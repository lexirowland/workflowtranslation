# @author: Lexi Rowland

# Workflow: average temperature in amsterdam.
# Workflow metadata (result and data sources)
_:wf3 a wf:Workflow;
	rdfs:comment "What is the average urban heat island (UHI) effect within each PC4 area in Amsterdam?"@en;	
	wf:source <https://maps.amsterdam.nl/open_geodata/?k=192>; #postcode areas	
	wf:source _:urbanheatislandtif; #urban heat island.tif
	wf:edge _:wf5_1, _:wf5_2, _:wf5_3.
	
_:wf5_1 wf:applicationOf <https://docs.qgis.org/3.10/en/docs/user_manual/processing_algs/qgis/vectoroverlay.html#clip>;
		wf:input1 <https://maps.amsterdam.nl/open_geodata/?k=192>; #noise map 
		wf:input2 _:urbanheatislandtif; 
		wf:output _:UHIams_tif. #fieldraster

_:wf5_2 wf:applicationOf <https://docs.qgis.org/3.10/en/docs/user_manual/processing_algs/qgis/rasteranalysis.html#zonal-statistics>;
		wf:input1 _:UHIams_tif;
		wf:output _:UHIzonalstatistics_shp.
		
_:wf5_3 wf:applicationOf <https://docs.qgis.org/3.10/en/docs/user_manual/processing_algs/qgis/vectorgeneral.html#join-attributes-by-field>;
		wf:input1 _:UHIzonalstatistics_shp;
		wf:input2 <https://maps.amsterdam.nl/open_geodata/?k=192>;
		wf:output _:meanUHIPC4_shp.
		

