@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix xml: <http://www.w3.org/XML/1998/namespace>.
@prefix wf: <http://geographicknowledge.de/vocab/Workflow.rdf#>.
@prefix tools: <http://geographicknowledge.de/vocab/GISTools.rdf#>.
@prefix arcpro: <https://pro.arcgis.com/en/pro-app/tool-reference/>.
@prefix pdok: <https://www.pdok.nl/introductie/-/article/>.

# @author: Lexi Rowland

# Workflow: noise pollution in amsterdam.
# Workflow metadata (result and data sources)
_:wf4 a wf:Workflow;
	rdfs:comment "Where in Amsterdam is noise pollution equal to or greater than 70dB?"@en;	
	wf:source <https://maps.amsterdam.nl/open_geodata/?k=192>; #postcode areas	
	wf:source <https://maps.amsterdam.nl/geluid>;	#Amsterdam noise map
	wf:edge _:wf4_1, _:wf4_2, _:wf4_3. 
	
_:wf4_1 wf:applicationOf <https://pro.arcgis.com/en/pro-app/tool-reference/conversion/polygon-to-raster.htm>; #ContourToFieldRaster
		wf:input1 <https://maps.amsterdam.nl/geluid>; #fieldQ, tessellationA, plainordinalA	
		wf:output _:noise_raster. #fieldQ, rasterA, plainordinalA
		
_:wf4_2 wf:applicationOf <https://pro.arcgis.com/en/pro-app/tool-reference/data-management/clip.htm>; #ClipRasterToLatticeExtentOrdinal
		wf:input1 <https://maps.amsterdam.nl/open_geodata/?k=192>; #objectQ, vectortessellationA, plainnominalA
		wf:input2 _:noise_raster; #fieldQ, rasterA, plainordinalA	
		wf:output _:noise_amsterdam. #fieldQ, rasterA, plainordinalA
		
_:wf4_3 wf:applicationOf <https://pro.arcgis.com/en/pro-app/tool-reference/spatial-analyst/raster-calculator.htm>; #CalculatorInequalitiesNominalOrdinal
		wf:input1 _:noise_amsterdam; #fieldQ, rasterA, plainordinalA		
		wf:output _:noisy_amsterdam. #fieldQ, rasterA, booleanA (existence raster)
		
		
