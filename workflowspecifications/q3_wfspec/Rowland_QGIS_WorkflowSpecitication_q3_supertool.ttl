@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix xml: <http://www.w3.org/XML/1998/namespace>.
@prefix wf: <http://geographicknowledge.de/vocab/Workflow.rdf#>.
@prefix tools: <http://geographicknowledge.de/vocab/GISTools.rdf#>.
@prefix arcpro: <https://pro.arcgis.com/en/pro-app/tool-reference/>.
@prefix pdok: <https://www.pdok.nl/introductie/-/article/>.
@prefix qtools: <http://geographicknowledge.de/vocab/QGISTools.rdf#>. #this needs to be checked with Simon. 

# @author: Lexi Rowland

# Workflow: accessibility of green space in Amsterdam.
# Workflow metadata (result and data sources)
_:wf3 a wf:Workflow;
	rdfs:comment "What is the accessibility of parks/green space for each PC4 area in Amsterdam?"@en;	
	wf:source <https://maps.amsterdam.nl/open_geodata/?k=192>; #postcode areas	
	wf:source <https://maps.amsterdam.nl/open_geodata/?k=152>; #land use map coverage, nominalA
	wf:edge _:wf3_1, _:wf3_2, _:wf3_3.
	
_:wf3_1 wf:applicationOf qtools:SelectAttributesCoverageAndExtractFeaturesExistenceVector; #supertool
	wf:input1 <https://maps.amsterdam.nl/open_geodata/?k=152>; #coverage
	wf:output _:parks2. #existence vector
	
_:wf3_2 wf:applicationOf qtools:RasteriseExistenceVectorAndProximityAnalysis; #supertool
	wf:input1 _:parks2; #existence vector, booleanA
	wf:output _:eudist. #fieldraster, ratioA
		
_:wf3_3 wf:applicationOf qtools:ZonalStatisticsMeanRatioJoin; #supertool
	wf:input1 _:eudist;
	wf:input2 <https://maps.amsterdam.nl/open_geodata/?k=192>; #postcode areas
	wf:output _:accessibility. #lattice, vectorA, ratioA
