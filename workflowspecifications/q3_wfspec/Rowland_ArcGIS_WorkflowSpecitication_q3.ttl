@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix xml: <http://www.w3.org/XML/1998/namespace>.
@prefix wf: <http://geographicknowledge.de/vocab/Workflow.rdf#>.
@prefix tools: <http://geographicknowledge.de/vocab/GISTools.rdf#>.
@prefix arcpro: <https://pro.arcgis.com/en/pro-app/tool-reference/>.
@prefix pdok: <https://www.pdok.nl/introductie/-/article/>.

# @author: Lexi Rowland

# Workflow: accessibility of green space in Amsterdam.
# Workflow metadata (result and data sources)
_:wf3 a wf:Workflow;
	rdfs:comment "What is the accessibility of parks/green space for each PC4 area in Amsterdam?"@en;	
	wf:source <https://maps.amsterdam.nl/open_geodata/?k=192>; #postcode areas	
	wf:source <https://maps.amsterdam.nl/open_geodata/?k=152>; #land use map 
	wf:edge _:wf3_1, _:wf3_2, _:wf3_3, _:wf3_4, _:wf3_5.
	
_:wf3_1 wf:applicationOf <https://desktop.arcgis.com/en/arcmap/10.3/tools/data-management-toolbox/select-layer-by-attribute.htm>;
		wf:input1 <https://maps.amsterdam.nl/open_geodata/?k=152>; #fieldQ, tessellationA, plainnominalA
		wf:output _:grondgebruik1. #fieldQ, tessellationA, plainnominalA
		
_:wf3_2 wf:applicationOf <https://desktop.arcgis.com/en/arcmap/10.3/tools/data-management-toolbox/copy-features.htm>;
		wf:input1 _:grondgebruik1; #fieldQ, tessellationA, plainnominalA
		wf:output _:parks2. #objectQ, existencevector, booleanA

_:wf3_3 wf:applicationOf <https://desktop.arcgis.com/en/arcmap/10.3/tools/spatial-analyst-toolbox/euclidean-distance.htm>;
		wf:input1 _:parks2; #objectQ, existencevector, booleanA
		wf:output _:eudist. #fieldQ, rasterA, plainratioA
		
_:wf3_4 wf:applicationOf <https://desktop.arcgis.com/en/arcmap/10.3/tools/spatial-analyst-toolbox/h-how-zonal-statistics-works.htm>; #implements tools:ZonalStatisticsMeanRatio
		wf:input1 _:eudist; #fieldQ, rasterA, ratioA
		wf:input2 <https://maps.amsterdam.nl/open_geodata/?k=192>; #objectQ, vectortessellationA, plainnominalA 
		wf:output _:accessibility. #objectQ, vectortessellationA, plainratioA
		
_:wf3_5 wf:applicationOf <https://desktop.arcgis.com/en/arcmap/10.3/tools/data-management-toolbox/add-join.htm>;
		wf:input1 _:accessibility; #objectQ, vectortessellationA, plainnominalA because the attribute of interest changes here
		wf:input2 <https://maps.amsterdam.nl/open_geodata/?k=192>; ##objectQ, vectortessellationA, plainnominalA 
		wf:output _:accessibility_shp. #objectQ, vectortessellationA, plainratioA 