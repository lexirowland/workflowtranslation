@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix xml: <http://www.w3.org/XML/1998/namespace>.
@prefix wf: <http://geographicknowledge.de/vocab/Workflow.rdf#>.
@prefix tools: <http://geographicknowledge.de/vocab/GISTools.rdf#>.
@prefix arcpro: <https://pro.arcgis.com/en/pro-app/tool-reference/>.
@prefix pdok: <https://www.pdok.nl/introductie/-/article/>.

# @author: Lexi Rowland

# Workflow: .
# Workflow metadata (result and data sources)
_:wf6 a wf:Workflow;
	rdfs:comment "What are suitable regions for building a sports complex, given several conditions, within the municipality of Utrecht?"@en;	
	wf:source _:buildg; #fieldraster, nominalA
	wf:source _:iswater; #existenceraster, booleanA
	wf:source _:isroad; #existenceraster, booleanA
	wf:source _:slope; #fieldraster, ratioA  	
	wf:edge _:wf6_1, _:wf6_2, _:wf6_3, _:wf6_4, _:wf6_5, _:wf6_6, _:wf6_7, _:wf6_8, _:wf6_9, _:wf6_10.
	
_:wf6_1 wf:applicationOf <https://desktop.arcgis.com/en/arcmap/10.3/tools/spatial-analyst-toolbox/reclassify.htm>;
		wf:input1 _:buildg; #fieldraster, nominalA
		wf:output _:industry. #existence raster, booleanA
		
_:wf6_2 wf:applicationOf <https://desktop.arcgis.com/en/arcmap/10.3/tools/spatial-analyst-toolbox/euclidean-distance.htm>;
		wf:input1 _:industry; #existence raster, booleanA
		wf:output _:inddist. #fieldraster, ratioA
		
_:wf6_3 wf:applicationOf <https://desktop.arcgis.com/en/arcmap/10.3/tools/spatial-analyst-toolbox/equal-to.htm>;
		wf:input1 _:buildg; #fieldraster, nominalA
		wf:output _:nobuild. #existenceraster, booleanA
		
_:wf6_4 wf:applicationOf <https://desktop.arcgis.com/en/arcmap/10.3/tools/spatial-analyst-toolbox/raster-calculator.htm>;
		wf:input1 _:slope; #fieldraster, ratioA
		wf:output _:nosteep. #existenceraster, booleanA
		
_:wf6_5 wf:applicationOf <https://desktop.arcgis.com/en/arcmap/10.3/tools/spatial-analyst-toolbox/raster-calculator.htm>;
		wf:input1 _:inddist; #fieldraster, ratioA
		wf:output _:noind. #existenceraster, booleanA
		
_:wf6_6 wf:applicationOf <https://desktop.arcgis.com/en/arcmap/10.3/tools/spatial-analyst-toolbox/raster-calculator.htm>;
		wf:input1 _:noind; #existenceraster, booleanA
		wf:input2 _:nobuild; #existenceraster, booleanA
		wf:input3 _:iswater; #existenceraster, booleanA
		wf:input4 _:isroad; #existenceraster, booleanA
		wf:input5 _:nosteep; #existenceraster, booleanA
		wf:output _:sportgo. #existenceraster, booleanA
		
_:wf6_7 wf:applicationOf <https://desktop.arcgis.com/en/arcmap/10.3/tools/spatial-analyst-toolbox/region-group.htm>;
		wf:input1 _:sportgo;
		wf:output _:sportclu. #fieldraster, nominalA
		
_:wf6_8 wf:applicationOf <https://desktop.arcgis.com/en/arcmap/10.3/tools/spatial-analyst-toolbox/set-null.htm>;
		wf:input1 _:sportclu;
		wf:input2 _:constantrasterfield;
		wf:output _:sportclub_nd. #fieldraster, nominalA
		
_:wf6_9 wf:applicationOf <https://desktop.arcgis.com/en/arcmap/10.3/tools/spatial-analyst-toolbox/zonal-geometry.htm>;
		wf:input1 _:sportclu_nd;
		wf:output _:sportarea. #fieldraster, ratioA
		
_:wf6_10 wf:applicationOf <https://desktop.arcgis.com/en/arcmap/10.3/tools/spatial-analyst-toolbox/raster-calculator.htm>;
		wf:input1 _:sportarea;
		wf:output _:sports. #existenceraster, booleanA 
		

