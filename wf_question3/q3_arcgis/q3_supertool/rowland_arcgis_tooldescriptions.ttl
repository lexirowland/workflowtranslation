########### Supertool: Adding and Calculating a Field 
tools:FullAddCalculateField tools:implements tools:AddCalculateFieldLatticeRatio;
	wf:edge _:wf1_1, _:wf1_2;
		wf:source _:in11;
	rdfs:label "Adding a field to an attribute table and calculating an input for that field based on a SQL expression. This is added to the attribute table of the existing dataset.".
#add field and calculate field are often seen as a combination tool when implemented for analysis. For the purpose of this annotation, it will be treated as one tool.
_:wf1_1 wf:applicationOf <https://desktop.arcgis.com/en/arcmap/latest/tools/data-management-toolbox/add-field.htm>;
	wf:input1 _:in11;
	wf:output _:out11.
_:wf1_2 wf:applicationOf <https://desktop.arcgis.com/en/arcmap/latest/tools/data-management-toolbox/calculate-field.htm>;
	wf:input1 _:out11;
	wf:output _:out12.

####for Lattice
tools:AddCalculateFieldLatticeRatio rdfs:label "Adds a new field to a region dataset and calculates the field's ratio scaled values based on a given expression.";
	wf:input1 [ a ccd:Lattice, ccd:VectorA ];
	wf:output [ a ccd:Lattice, ccd:VectorA, ccd:RatioA ].

########### Supertool: Zonal Statistics and Join Tool
tools:JoinToZonalStatisticsTable tools:implements tools:ZonalStatisticsMeanIntervalPointJoin, tools:ZonalStatisticsMeanRatioJoin, tools:ZonalStatisticsSumRatioJoin;  
	wf:edge _:wf2_1, _:wf2_2;
		wf:source _:in21, _:in22;
	rdfs:label "Performs a zonal statistics related calculation and joins the output table to the original input dataset.".
_:wf2_1 wf:ApplicationOf <https://desktop.arcgis.com/en/arcmap/latest/tools/spatial-analyst-toolbox/zonal-statistics-as-table.htm>;
	wf:input1 _:in21;
	wf:input2 _:in22;
	wf:output _:out21.
_:wf2_2 wf:ApplicationOf <https://desktop.arcgis.com/en/arcmap/latest/tools/data-management-toolbox/join-field.htm>;
	wf:input1 _:out21;
	wf:input2 _:in22;
	wf:output _:out22.
	
tools:ZonalStatisticsMeanRatioJoin rdfs:label "Calculates the average value on the values with a ratio measurement level of a raster within the zones of another dataset and then joins the output table to the original vector dataset";
	wf:input1 [ a ccd:FieldRaster, ccd:RatioA ];
	wf:input2 [ a ccd:Lattice, ccd:VectorA ];
	wf:output [ a ccd:Lattice, ccd:VectorA, ccd:RatioA ].
		
tools:ZonalStatisticsSumRatioJoin rdfs:label "Calculates the sum value on the values with a ratio measurement level of a raster within the zones of another dataset and then joins the output table to the original vector dataset";
	wf:input1 [ a ccd:FieldRaster, ccd:RatioA ];
	wf:input2 [ a ccd:Lattice, ccd:VectorA ];
	wf:output [ a ccd:Lattice, ccd:VectorA, ccd:RatioA ].
	
tools:ZonalStatisticsMeanIntervalPointJoin rdfs:label "Calculates the sum value on the values with a ratio measurement level of a raster within the zones of another dataset and then joins the output table to the original vector dataset";
	wf:input1 [ a ccd:PointMeasures, ccd:IntervalA ];
	wf:input2 [ a ccd:Lattice, ccd:VectorA ];
	wf:output [ a ccd:Lattice, ccd:VectorA, ccd:RatioA ].

